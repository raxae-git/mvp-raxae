<div class="row" style="margin-top: 100px">
  <div class="col-6">
    <%= form_with model:[:backoffice, @beta_user], local: true do |f| %>
    <div class="form-group">
      <%= label_tag :name %>
      <%= f.text_field :name, class:"form-control" %>
    </div>
    <div class="form-group">
      <%= label_tag :email %>
      <%= f.email_field :email, class:"form-control" %>
    </div>
    <div class="row">
      <div class="form-group col-6">
        <%= label_tag :messaging_service %>
        <%= f.email_field :messaging_service, class:"form-control" %>
      </div>
      <div class="form-group col-6">
        <%= label_tag :phone %>
        <%= f.email_field :phone, class:"form-control" %>
      </div>
    </div>
    <div class="form-group">
      <%= label_tag :service_of_interest %>
      <%= f.email_field :service_of_interest, class:"form-control" %>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
    <% end %>
  </div>
</div>